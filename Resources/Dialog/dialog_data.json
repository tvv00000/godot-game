	"npc_botanic": {
		"trees": [
			{
				"branch_id": "npc_botanic_1",
				"dialogs": [
					{
						"state": "start",
						"text": "Ohoh, tervist Eksmati!",
						"options": {
							"Kas sa võiksid mulle tüümiani kasvutingimustest rääkida?": "plant_info",
							"Kas ma saaksin sind kuidagi aidata?": "give_quests",
							"Hüvasti!": "exit"
						}
					},{
						"state": "plant_info",
						"text": "Ta pigem naudib väikest kannatust – põud pole talle probleem. Vahemeremaades, kust ta pärineb, seal meeldib talle kivide vahel ronida. Ta on väike vapper ürditaimeke.",
						"options": {
							"Ürditaimeke? See mulle meeldib! Tänan sind, valguse ja kivide valitseja!": "start",
						}
					},{
						"state": "give_quests",
						"text": "Jah, ikka saad! Mine leia kolm tüümianitaime ning too need mulle.",
						"options": {
							"Lähen otsima!": "end"
						}
					},{
						"state": "end",
						"text": "",
						"options": {}
					},{
						"state": "exit",
						"text": "",
						"options": {}
					}
				]
			},{
				"branch_id": "npc_botanic_2",
				"dialogs": [
					{
						"state": "start",
						"text": "Oh, Eksmati, juba tagasi?",
						"options": {
							"Kas võiksid mulle veel natuke tüümianist rääkida?": "plant_info",
							"Hüvasti!": "exit"
						}
					},{
						"state": "plant_info",
						"text": "Ikka! tüümian ehk aed-liivatee on tõeline päikeselaps! Ta tahab päikest nagu nartsiss kevadel – palju valgust, kuiva ja kivist pinnast",
						"options": {
							"Et siis porilompides ta ei taha hullata, nagu mõni järvetaim?": "plant_info2",
						}
					},{
						"state": "plant_info2",
						"text": "Täpselt! Ta pigem naudib väikest kannatust – põud pole talle probleem. Vahemeremaades, kust ta pärineb, seal meeldib talle kivide vahel ronida. Ta on väike vapper ürditaimeke.",
						"options": {
							"Ürditaimeke? Selge, aitäh!": "start",
							"Selge, hüvasti": "exit"
						}
					},{
						"state": "end",
						"text": "",
						"options": {}
					},{
						"state": "exit",
						"text": "",
						"options": {} 
					}
				]
			},{
			"branch_id": "npc_default",
