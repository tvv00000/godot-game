	"npc_botanic": {
		"trees": [
			{
				"branch_id": "npc_botanic_1",
				"dialogs": [
					{
						"state": "start",
						"text": "Oo, Eksmati",
						"options": {
							"Kas sa võid mulle tüümia": "plant_info",
							"Kas ma saan teda kuidagi aidata?": "give_quests",
							"Hüvasti": "exit"
						}
					},{
						"state": "plant_info",
						"text": "Täpselt! Ta pigem naudib väikest kannatust – põud pole talle probleem. Vahemeremaades, kust ta pärineb, seal meeldib talle kivide vahel ronida. Ta on väike vapper ürditaimeke.",
						"options": {
							"Ürditaimeke? Mulle meeldib see! Tänan sind, valguse ja kivide valitseja!": "start",
						}
					},{
						"state": "give_quests",
						"text": "Ja ikka saad, too mulle 3 tüümiani.",
						"options": {
							"Lähen otsima!": "end"
						}
					},{
						"state": "end",
						"text": "",
						"options": {}
					},{
						"state": "exit",
						"text": "",
						"options": {}
					}
				]
			},{
				"branch_id": "npc_botanic_2",
				"dialogs": [
					{
						"state": "start",
						"text": "Oh, Eksmati, juba tagasi?",
						"options": {
							"Kas võid uuesti tüümianist rääkida?": "plant_info",
							"Kas oleks veel midagi mida teha?": "give_quest",
							"Hüvasti": "exit"
						}
					},{
						"state": "plant_info",
						"text": "Ikka! tüümian ehk aed-liivatee on tõeline päikeselaps! Ta tahab päikest nagu nartsiss kevadel – palju valgust, kuiva ja kivist pinnast",
						"options": {
							"Et siis porilompides ta ei taha hullata, nagu mõni järvetaim?": "plant_info2",
						}
					},{
						"state": "plant_info2",
						"text": "Täpselt! Ta pigem naudib väikest kannatust – põud pole talle probleem. Vahemeremaades, kust ta pärineb, seal meeldib talle kivide vahel ronida. Ta on väike vapper ürditaimeke.",
						"options": {
							"Ürditaimeke? Selge, aitäh!": "start",
							"Selge, hüvasti": "exit"
						}
					},{
						"state": "give_quest",
						"text": "Nüüd kui sa sellest räägid, mine otsi taimetark lembit üles. Kui sa ta leiad siis küsi talt tüümiani kohta",
						"options": {
							"Lahen otsin ta üles": "end",
							"Ei saa praegu": "exit"
						}
					},{
						"state": "end",
						"text": "",
						"options": {}
					},{
						"state": "exit",
						"text": "",
						"options": {}
					}
				]
			},{
				"branch_id": "npc_botanic_3",
				"dialogs": [
					{
						"state": "start",
						"text": "Tsau Eksmati, kuidas läheb",
						"options": {
							"Kas võid uuesti tüümianist rääkida?": "plant_info",
							"Kus taimetark Lembit oli?": "npc_info",
							"Hüvasti": "exit"
						}
					},{
						"state": "plant_info",
						"text": "Ikka! tüümian ehk aed-liivatee on tõeline päikeselaps! Ta tahab päikest nagu nartsiss kevadel – palju valgust, kuiva ja kivist pinnast",
						"options": {
							"Et siis porilompides ta ei taha hullata, nagu mõni järvetaim?": "plant_info2",
						}
					},{
						"state": "plant_info2",
						"text": "Täpselt! Ta pigem naudib väikest kannatust – põud pole talle probleem. Vahemeremaades, kust ta pärineb, seal meeldib talle kivide vahel ronida. Ta on väike vapper ürditaimeke.",
						"options": {
							"Mul on veel küsimusi.": "start",
							"Selge, hüvasti": "exit"
						}
					},{
						"state": "npc_info",
						"text": "Ta peaks kuskil üleval olema",
						"options": {
							"Mul on veel küsimusi": "start",
							"Selge, aitäh": "exit"
						}
					},{
						"state": "end",
